{
  "name": "xprocess",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test umi dev",
    "build": "umi build",
    "build:sandbox": "cross-env UMI_ENV=sandbox umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "test": "umi test",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@rematch/core": "^1.2.0",
    "@rematch/loading": "^1.2.1",
    "@rematch/persist": "^1.1.6",
    "antd": "^3.15.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.1",
    "react-intl-universal": "^2.2.2",
    "react-redux": "^7.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "umi": "^2.6.1",
    "umi-plugin-react": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
